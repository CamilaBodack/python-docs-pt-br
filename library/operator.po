msgstr ""

#: ../../library/operator.rst:414
msgid "``a = irshift(a, b)`` is equivalent to ``a >>= b``."
msgstr "``a = irshift(a, b)`` é equivalente a ``a >>= b``."

#: ../../library/operator.rst:422
